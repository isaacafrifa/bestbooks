type Query {
    allBooks: [Book]
    findBook(id: ID): Book
    allPaginatedBooks(page: Int, size: Int): [Book]
}

type Book {
    id: ID!
    title: String!
    rating: Rating
    author: Author
}

type Author {
    id: ID!
    firstName: String
    lastName: String
    books: [Book]
}

type Rating {
    rating: String
    star: String
}